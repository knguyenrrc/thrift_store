<div class="container">
  <h3 class="title is-3">All Publishers:</h3>
  <nav class="pagination" role="navigation" aria-label="pagination"><%= paginate @publishers %> </nav>
  <ol>
    <% @publishers.each do |publisher| %>
      <li>
        <%= link_to publisher.name, publisher %>
      </li>
    <% end %>
  </ol>
  <nav class="pagination" role="navigation" aria-label="pagination"><%= paginate @publishers %> </nav>
</div>